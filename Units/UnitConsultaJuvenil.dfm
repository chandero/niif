  object IBQuery2: TIBQuery
    Database = dmGeneral.IBDatabase1
    Transaction = dmGeneral.IBTransaction1
    SQL.Strings = (
      'SELECT '
      '  "cap$tipocaptacion".ID_TIPO_CAPTACION,'
      '  "cap$tipocaptacion".DESCRIPCION'
      'FROM'
      '  "cap$tipocaptacion"'
      
        '  INNER JOIN "cap$tiposforma" ON ("cap$tipocaptacion".ID_FORMA =' +
        ' "cap$tiposforma".ID_FORMA)'
      'WHERE'
      ' ("cap$tiposforma".AHORRO <> 0) or'
      ' ("cap$tiposforma".PROGRAMADO <> 0) or'
      ' ("cap$tiposforma".CERTIFICADO <> 0 )')
    Left = 692
    Top = 36
  end
  object IBQuery1: TIBQuery
    Database = dmGeneral.IBDatabase1
    Transaction = dmGeneral.IBTransaction1
    SQL.Strings = (
      'SELECT '
      
        '  "gen$persona".PRIMER_APELLIDO || '#39' '#39' || "gen$persona".SEGUNDO_' +
        'APELLIDO || '#39' '#39' || "gen$persona".NOMBRE AS NOMBRE,'
      '  "cap$segurojuvenil".ID_PERSONA,'
      '  "cap$segurojuvenil".EDAD,'
      '  "cap$segurojuvenil".VALOR_ASEGURADO,'
      '  "cap$segurojuvenil".FECHA_SEGURO,'
      '  "cap$polizajuvenil".POLIZA,'
      '  "cap$segurojuvenil".NUMERO_CUENTA,'
      '  "cap$segurojuvenil".PERSONA,'
      '  "cap$segurojuvenil".ID_IDENTIFICACION'
      'FROM'
      '  "cap$segurojuvenil"'
      
        '  LEFT OUTER JOIN "gen$persona" ON ("cap$segurojuvenil".ID_IDENT' +
        'IFICACION = "gen$persona".ID_IDENTIFICACION)'
      
        '  AND ("cap$segurojuvenil".ID_PERSONA = "gen$persona".ID_PERSONA' +
        ')'
      
        'LEFT OUTER JOIN "cap$polizajuvenil" ON ("cap$segurojuvenil".ID_P' +
        'OLIZA = "cap$polizajuvenil".ID_POLIZA)'
      'WHERE'
      '  "cap$segurojuvenil".FECHA_SEGURO BETWEEN :FECHA1 AND :FECHA2 '
      
        'AND "gen$persona".PRIMER_APELLIDO || '#39' '#39' || "gen$persona".SEGUND' +
        'O_APELLIDO || '#39' '#39' || "gen$persona".NOMBRE  LIKE '#39'%'#39' || :DESCRIPC' +
        'ION || '#39'%'#39)
    Left = 688
    Top = 152
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'FECHA1'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'FECHA2'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'DESCRIPCION'
        ParamType = ptUnknown
      end>
  end
