  object IBQuery2: TIBQuery
    Database = dmGeneral.IBDatabase1
    Transaction = dmGeneral.IBTransaction1
    SQL.Strings = (
      'SELECT '
      '  "cap$maestrotitular".ID_PERSONA'
      'FROM'
      ' "cap$maestrotitular"'
      
        ' INNER JOIN "col$credivida" ON ("cap$maestrotitular".NUMERO_CUEN' +
        'TA="col$credivida".NUMERO_CUENTA)'
      
        '  AND ("cap$maestrotitular".DIGITO_CUENTA="col$credivida".DG_CUE' +
        'NTA)'
      'WHERE'
      '  ("cap$maestrotitular".ID_TIPO_CAPTACION = 2) AND '
      '  ("col$credivida".FECHA_VENCIMIENTO = :FECHA) AND '
      '  ("cap$maestrotitular".ID_PERSONA = :ID)')
    Left = 288
    Top = 40
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'FECHA'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
